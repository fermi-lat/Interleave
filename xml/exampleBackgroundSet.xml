<sourceList xmlns='http://xml.netbeans.org/examples/targetNS'
  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
  xsi:schemaLocation='http://xml.netbeans.org/examples/targetNS file:/./backgroundInterleave.xsd'>

   <source name="earthsLimb" type="PtSCzenith" rangeUnits="degrees" rangeMin="0.0" rangeMax="180.0">
      <bin min="-1.0" max="10." triggerRate="100.0" downlinkRate="0.1">
         <fileList>
            <file filePath="$(ROOTTESTDATAROOT)/data/default/merit.root" treeName="MeritTuple" numEvents="5"/>
         </fileList>
      </bin>
      <bin min="10.0" max="20.0" triggerRate="100.0" downlinkRate="0.1">
         <fileList>
            <file filePath="$(ROOTTESTDATAROOT)/data/default/merit.root" treeName="MeritTuple" numEvents="5"/>
         </fileList>
      </bin>
      <bin min="20.0" max="180." triggerRate="100.0" downlinkRate="0.1">
         <fileList>
            <file filePath="$(ROOTTESTDATAROOT)/data/default/merit.root" treeName="MeritTuple" numEvents="5"/>
         </fileList>
      </bin>
   </source>

   <source name="diffuse" type="PtMagLat" rangeUnits="degrees" rangeMin="-45" rangeMax="45.0">
      <bin min="-45." max="45.0" triggerRate="3000.0" downlinkRate="0.2">
         <fileList>
            <file filePath="$(ROOTTESTDATAROOT)/data/default/merit.root" treeName="MeritTuple" numEvents="5"/>
         </fileList>
      </bin>
   </source>      
</sourceList>
